{
	"name": "greenpeace/planet4-greenland",
	"description": "Greenpeace P4 Greenland",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-child-theme-greenland" : "0.*",
		"greenpeace/planet4-plugin-migrated-urls": "0.0.1"
	},
	"scripts": {
		"core:language": "wp language core install da_DK; wp language core activate da_DK",
		"core:language-kl" : "rsync -ar extra/languages/kl.* public/wp-content/languages/",
		"install:plugin-wpml": "wp plugin install --activate https://raw.githubusercontent.com/greenpeace/planet4-3rdparty-plugins/master/sitepress-multilingual-cms.4.3.4.zip",
		"install:plugin-wpml-management": "wp plugin install --activate https://raw.githubusercontent.com/greenpeace/planet4-3rdparty-plugins/master/wpml-translation-management.2.9.2.zip",
		"install:plugin-wpml-stringtranslate": "wp plugin install --activate https://raw.githubusercontent.com/greenpeace/planet4-3rdparty-plugins/master/wpml-string-translation.3.0.4.zip",
		"install:plugin-wpimport": "wp plugin install --activate https://raw.githubusercontent.com/greenpeace/planet4-3rdparty-plugins/master/wp-all-import-pro_4.5.4.zip",

		
		"site:custom" : [
			"@core:language",
			"@core:language-kl",
			"@install:plugin-wpml",
			"@install:plugin-wpml-management",
			"@install:plugin-wpml-stringtranslate",
			"@install:plugin-wpimport"
		]
	}
}
